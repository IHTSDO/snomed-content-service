#search a term in description labels and retrieves first 20 concept which matches search criteria
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix owl: <http://www.w3.org/2002/07/owl#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
prefix sn: <http://snomed.info/sct/term/>
PREFIX text: <http://jena.apache.org/text#>


SELECT ?s ?desc ?dLabel
 
WHERE 
{

	?s sn:description ?desc.
    ?desc text:query (rdfs:label 'body Structure' 10) .	
    ?desc rdfs:label ?dLabel.
    
} 

LIMIT 20